<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lehu.mychat">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.lehu.mychat.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.lehu.mychat.LoginActivity"
            android:parentActivityName="com.lehu.mychat.StartActivity" />

        <activity android:name="com.lehu.mychat.MainActivity" />

        <activity
            android:name="com.lehu.mychat.RegisterActivity"
            android:parentActivityName="com.lehu.mychat.StartActivity" />

    </application>

</manifest>